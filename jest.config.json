{
    "verbose": true,
    "roots": [
        "<rootDir>"
    ],
    "setupFilesAfterEnv": [
        "<rootDir>/test/setupTests.ts"
    ],
    "transform": {
        "\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*\\.test|(\\.|/)test)\\.[jt]sx?$",
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "node"
    ],
    "moduleNameMapper": {
        "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [],
    "reporters": [
        "default"
    ]
  }